apiVersion: monitoring.coreos.com/v1
kind: ServiceMonitor
metadata:
  name: dev-back-monitor
  labels:
    release: monitoring
spec:
  selector:
    matchLabels:
      project: app-back
  endpoints:
  - port: tcp
---
